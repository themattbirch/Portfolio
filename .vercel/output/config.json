{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/about\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/category\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/contact\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/projects\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/rss\\.xml\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/skills\\/soft-skills\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/skills\\/technical-skills\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}